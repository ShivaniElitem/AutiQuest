<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/purple"
    tools:context=".ThirdActivity">

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Questionnaire"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="italic|bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.1" />

    <View
        android:id="@+id/colored_bar1"
        android:layout_width="325sp"
        android:layout_height="130sp"
        android:layout_marginTop="10dp"
        android:background="@drawable/border"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/title" />

    <View
        android:id="@+id/colored_bar2"
        android:layout_width="325sp"
        android:layout_height="130sp"
        android:background="@drawable/border"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/colored_bar1"/>

    <View
        android:id="@+id/colored_bar3"
        android:layout_width="325sp"
        android:layout_height="130sp"
        android:background="@drawable/border"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/colored_bar2"/>

    <View
        android:id="@+id/colored_bar4"
        android:layout_width="325sp"
        android:layout_height="130sp"
        android:layout_marginTop="10dp"
        android:background="@drawable/border"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/colored_bar3" />

    <Button
        android:id="@+id/myButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit"
        android:textColor="@color/black"
        android:backgroundTint ="@color/purple_200"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintTop_toBottomOf="@id/colored_bar4" />


    <TextView
        android:id="@+id/rec1text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="What is the kids name?"
        android:textStyle="bold"
        android:textSize="10sp"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/colored_bar1"
        app:layout_constraintEnd_toEndOf="@id/colored_bar1"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintTop_toTopOf="@id/colored_bar1"
        app:layout_constraintBottom_toBottomOf="@id/colored_bar1"
        app:layout_constraintVertical_bias="0.1" />

    <EditText
        android:id="@+id/firstname"
        android:layout_width="75dp"
        android:layout_height="20dp"
        android:hint="First Name"
        android:textSize="12sp"
        android:textColor="@color/black"
        android:textColorHint="@color/grey"
        android:textCursorDrawable="@null"
        android:gravity="center_horizontal"
        android:background="@drawable/border1"
        app:layout_constraintStart_toEndOf="@id/rec1text1"
        app:layout_constraintEnd_toEndOf="@id/colored_bar1"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintTop_toTopOf="@id/rec1text1"
        app:layout_constraintBottom_toBottomOf="@id/rec1text1"/>

    <EditText
        android:id="@+id/lastname"
        android:layout_width="75dp"
        android:layout_height="20dp"
        android:hint="Last Name"
        android:textSize="12sp"
        android:background="@drawable/border1"
        android:textColor="@color/black"
        android:textColorHint="@color/grey"
        android:textCursorDrawable="@null"
        android:gravity="center_horizontal"
        app:layout_constraintStart_toEndOf="@id/rec1text1"
        app:layout_constraintEnd_toEndOf="@id/colored_bar1"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintTop_toTopOf="@id/rec1text1"
        app:layout_constraintBottom_toBottomOf="@id/rec1text1"/>

    <TextView
        android:id="@+id/rec1text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="What is the kids birthday?"
        android:textStyle="bold"
        android:textSize="10sp"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/colored_bar1"
        app:layout_constraintEnd_toEndOf="@id/colored_bar1"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintTop_toTopOf="@id/colored_bar1"
        app:layout_constraintBottom_toBottomOf="@id/colored_bar1"
        app:layout_constraintVertical_bias="0.3" />

    <EditText
        android:id="@+id/month"
        android:layout_width="30dp"
        android:layout_height="20dp"
        android:hint="MM"
        android:textSize="12sp"
        android:background="@drawable/border1"
        android:textColor="@color/black"
        android:textColorHint="@color/grey"
        android:textCursorDrawable="@null"
        android:gravity="center_horizontal"
        app:layout_constraintStart_toEndOf="@id/rec1text2"
        app:layout_constraintEnd_toEndOf="@id/colored_bar1"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintTop_toTopOf="@id/rec1text2"
        app:layout_constraintBottom_toBottomOf="@id/rec1text2"/>

    <TextView
        android:id="@+id/backslash1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="/"
        android:textStyle="bold"
        android:textSize="20sp"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@id/month"
        app:layout_constraintEnd_toStartOf="@id/day"
        app:layout_constraintTop_toTopOf="@id/month"
        app:layout_constraintBottom_toBottomOf="@id/month" />

    <EditText
        android:id="@+id/day"
        android:layout_width="30dp"
        android:layout_height="20dp"
        android:hint="DD"
        android:textSize="12sp"
        android:background="@drawable/border1"
        android:textColor="@color/black"
        android:textColorHint="@color/grey"
        android:textCursorDrawable="@null"
        android:gravity="center_horizontal"
        app:layout_constraintStart_toEndOf="@id/rec1text2"
        app:layout_constraintEnd_toEndOf="@id/colored_bar1"
        app:layout_constraintHorizontal_bias="0.45"
        app:layout_constraintTop_toTopOf="@id/rec1text2"
        app:layout_constraintBottom_toBottomOf="@id/rec1text2"/>

    <TextView
        android:id="@+id/backslash2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="/"
        android:textStyle="bold"
        android:textSize="20sp"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@id/day"
        app:layout_constraintEnd_toStartOf="@id/year"
        app:layout_constraintTop_toTopOf="@id/month"
        app:layout_constraintBottom_toBottomOf="@id/month" />

    <EditText
        android:id="@+id/year"
        android:layout_width="40dp"
        android:layout_height="20dp"
        android:hint="YYYY"
        android:textSize="12sp"
        android:background="@drawable/border1"
        android:textColor="@color/black"
        android:textColorHint="@color/grey"
        android:textCursorDrawable="@null"
        android:gravity="center_horizontal"
        app:layout_constraintStart_toEndOf="@id/rec1text2"
        app:layout_constraintEnd_toEndOf="@id/colored_bar1"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintTop_toTopOf="@id/rec1text2"
        app:layout_constraintBottom_toBottomOf="@id/rec1text2"/>

    <TextView
        android:id="@+id/rec1text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Where are you located?"
        android:textStyle="bold"
        android:textSize="10sp"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/colored_bar1"
        app:layout_constraintEnd_toEndOf="@id/colored_bar1"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintTop_toTopOf="@id/colored_bar1"
        app:layout_constraintBottom_toBottomOf="@id/colored_bar1"
        app:layout_constraintVertical_bias="0.55" />

    <EditText
        android:id="@+id/location"
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:hint="Location"
        android:textSize="12sp"
        android:textColor="@color/black"
        android:textColorHint="@color/grey"
        android:textCursorDrawable="@null"
        android:gravity="center_horizontal"
        android:background="@drawable/border1"
        app:layout_constraintStart_toEndOf="@id/rec1text3"
        app:layout_constraintEnd_toEndOf="@id/colored_bar1"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintTop_toTopOf="@id/rec1text3"
        app:layout_constraintBottom_toBottomOf="@id/rec1text3"/>


    <TextView
        android:id="@+id/rec1text4"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Has your child ever gone through ABA Therapy?"
        android:textColor="@color/black"
        android:textSize="10sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/colored_bar1"
        app:layout_constraintEnd_toEndOf="@id/colored_bar1"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toEndOf="@+id/colored_bar1"
        app:layout_constraintStart_toStartOf="@id/colored_bar1"
        app:layout_constraintTop_toTopOf="@id/colored_bar1"
        app:layout_constraintVertical_bias="0.8" />


    <RadioButton
        android:id="@+id/yes1"
        android:layout_width="60dp"
        android:layout_height="20dp"
        android:scaleX="0.6"
        android:scaleY="0.6"
        android:text="Yes"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/rec1text4"
        app:layout_constraintEnd_toEndOf="@id/colored_bar1"
        app:layout_constraintHorizontal_bias="0.154"
        app:layout_constraintStart_toEndOf="@id/rec1text4"
        app:layout_constraintTop_toTopOf="@id/rec1text4"
        app:layout_constraintVertical_bias="1.0" />

    <RadioButton
        android:id="@+id/no1"
        android:layout_width="60dp"
        android:layout_height="20dp"
        android:scaleX="0.6"
        android:scaleY="0.6"
        android:text="No"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/rec1text4"
        app:layout_constraintEnd_toEndOf="@id/colored_bar1"
        app:layout_constraintHorizontal_bias="0.855"
        app:layout_constraintStart_toEndOf="@id/rec1text4"
        app:layout_constraintTop_toTopOf="@id/rec1text4"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/rec2text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="What behaviors would you like to work on?"
        android:textStyle="bold"
        android:textSize="10sp"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/colored_bar2"
        app:layout_constraintEnd_toEndOf="@id/colored_bar2"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintTop_toTopOf="@id/colored_bar2"
        app:layout_constraintBottom_toBottomOf="@id/colored_bar2"
        app:layout_constraintVertical_bias="0.05" />

    <RadioButton
        android:id="@+id/q1op1"
        android:layout_width="400dp"
        android:layout_height="20dp"
        android:scaleX="0.6"
        android:scaleY="0.6"
        android:text="Aggression (hitting, scratching, or biting others)"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@id/colored_bar2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@id/colored_bar2"
        app:layout_constraintTop_toBottomOf="@id/rec2text1" />

    <RadioButton
        android:id="@+id/q1op2"
        android:layout_width="400dp"
        android:layout_height="20dp"
        android:scaleX="0.6"
        android:scaleY="0.6"
        android:text="Self-harm (hiting or biting self)"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@id/colored_bar2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@id/colored_bar2"
        app:layout_constraintTop_toBottomOf="@id/q1op1" />

    <RadioButton
        android:id="@+id/q1op3"
        android:layout_width="400dp"
        android:layout_height="20dp"
        android:scaleX="0.6"
        android:scaleY="0.6"
        android:text="Destruction (throwing or breaking things)"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@id/colored_bar2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@id/colored_bar2"
        app:layout_constraintTop_toBottomOf="@id/q1op2" />

    <RadioButton
        android:id="@+id/q1op4"
        android:layout_width="400dp"
        android:layout_height="20dp"
        android:scaleX="0.6"
        android:scaleY="0.6"
        android:text="Pica (eating inedible things)"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@id/colored_bar2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@id/colored_bar2"
        app:layout_constraintTop_toBottomOf="@id/q1op3" />

    <RadioButton
        android:id="@+id/q1op5"
        android:layout_width="400dp"
        android:layout_height="20dp"
        android:scaleX="0.6"
        android:scaleY="0.6"
        android:text="Elopment (running away or wandering off)"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@id/colored_bar2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@id/colored_bar2"
        app:layout_constraintTop_toBottomOf="@id/q1op4" />

    <TextView
        android:id="@+id/rec3text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="What are some stressors?"
        android:textStyle="bold"
        android:textSize="10sp"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/colored_bar3"
        app:layout_constraintEnd_toEndOf="@id/colored_bar3"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintTop_toTopOf="@id/colored_bar3"
        app:layout_constraintBottom_toBottomOf="@id/colored_bar3"
        app:layout_constraintVertical_bias="0.05" />

    <RadioButton
        android:id="@+id/q2op1"
        android:layout_width="400dp"
        android:layout_height="20dp"
        android:scaleX="0.6"
        android:scaleY="0.6"
        android:text="Communication"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@id/colored_bar2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@id/colored_bar2"
        app:layout_constraintTop_toBottomOf="@id/rec3text1" />

    <RadioButton
        android:id="@+id/q2op2"
        android:layout_width="400dp"
        android:layout_height="20dp"
        android:scaleX="0.6"
        android:scaleY="0.6"
        android:text="Social Cues"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@id/colored_bar2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@id/colored_bar2"
        app:layout_constraintTop_toBottomOf="@id/q2op1" />

    <RadioButton
        android:id="@+id/q2op3"
        android:layout_width="400dp"
        android:layout_height="20dp"
        android:scaleX="0.6"
        android:scaleY="0.6"
        android:text="Sensory Environment"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@id/colored_bar2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@id/colored_bar2"
        app:layout_constraintTop_toBottomOf="@id/q2op2" />

    <RadioButton
        android:id="@+id/q2op4"
        android:layout_width="400dp"
        android:layout_height="20dp"
        android:scaleX="0.6"
        android:scaleY="0.6"
        android:text="Routine Changes"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@id/colored_bar2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@id/colored_bar2"
        app:layout_constraintTop_toBottomOf="@id/q2op3" />

    <RadioButton
        android:id="@+id/q2op5"
        android:layout_width="400dp"
        android:layout_height="20dp"
        android:scaleX="0.6"
        android:scaleY="0.6"
        android:text="Anxiety"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@id/colored_bar2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@id/colored_bar2"
        app:layout_constraintTop_toBottomOf="@id/q2op4" />

    <TextView
        android:id="@+id/rec4text1"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:text="Does your child use verbal or non-verbal communication?"
        android:textColor="@color/black"
        android:textSize="10sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/colored_bar4"
        app:layout_constraintEnd_toEndOf="@id/colored_bar4"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toEndOf="@+id/colored_bar4"
        app:layout_constraintStart_toStartOf="@+id/colored_bar4"
        app:layout_constraintTop_toTopOf="@id/colored_bar4"
        app:layout_constraintVertical_bias="0.1" />

    <RadioButton
        android:id="@+id/verbal"
        android:layout_width="60dp"
        android:layout_height="20dp"
        android:scaleX="0.6"
        android:scaleY="0.6"
        android:text="Ver"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/rec4text1"
        app:layout_constraintEnd_toEndOf="@id/colored_bar1"
        app:layout_constraintHorizontal_bias="0.147"
        app:layout_constraintStart_toEndOf="@id/rec4text1"
        app:layout_constraintTop_toTopOf="@id/rec4text1"
        app:layout_constraintVertical_bias="0.0" />

    <RadioButton
        android:id="@+id/nonverbal"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:scaleX="0.6"
        android:scaleY="0.6"
        android:text="No-Ver"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/rec4text1"
        app:layout_constraintEnd_toEndOf="@id/colored_bar1"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@id/rec4text1"
        app:layout_constraintTop_toTopOf="@id/rec4text1"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/rec4text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="10dp"
        android:text="Does your child attend school in-person?"
        android:textColor="@color/black"
        android:textSize="10sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@id/colored_bar4"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="@+id/colored_bar4"
        app:layout_constraintTop_toBottomOf="@id/rec4text1" />

    <RadioButton
        android:id="@+id/yes2"
        android:layout_width="60dp"
        android:layout_height="20dp"
        android:scaleX="0.6"
        android:scaleY="0.6"
        android:text="Yes"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/rec4text2"
        app:layout_constraintEnd_toEndOf="@id/colored_bar1"
        app:layout_constraintHorizontal_bias="0.154"
        app:layout_constraintStart_toEndOf="@id/rec4text2"
        app:layout_constraintTop_toTopOf="@id/rec4text2"
        app:layout_constraintVertical_bias="1.0" />

    <RadioButton
        android:id="@+id/no2"
        android:layout_width="60dp"
        android:layout_height="20dp"
        android:scaleX="0.6"
        android:scaleY="0.6"
        android:text="No"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/rec4text2"
        app:layout_constraintEnd_toEndOf="@id/colored_bar1"
        app:layout_constraintHorizontal_bias="0.855"
        app:layout_constraintStart_toEndOf="@id/rec4text2"
        app:layout_constraintTop_toTopOf="@id/rec4text2"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/rec4text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="7dp"
        android:layout_marginTop="10dp"
        android:text="Does your child have dietary restrictions?"
        android:textColor="@color/black"
        android:textSize="10sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@id/colored_bar4"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="@+id/colored_bar4"
        app:layout_constraintTop_toBottomOf="@id/rec4text2" />

    <RadioButton
        android:id="@+id/yes3"
        android:layout_width="60dp"
        android:layout_height="20dp"
        android:scaleX="0.6"
        android:scaleY="0.6"
        android:text="Yes"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/rec4text3"
        app:layout_constraintEnd_toEndOf="@id/colored_bar1"
        app:layout_constraintHorizontal_bias="0.154"
        app:layout_constraintStart_toEndOf="@id/rec4text3"
        app:layout_constraintTop_toTopOf="@id/rec4text3"
        app:layout_constraintVertical_bias="1.0" />

    <RadioButton
        android:id="@+id/no3"
        android:layout_width="60dp"
        android:layout_height="20dp"
        android:scaleX="0.6"
        android:scaleY="0.6"
        android:text="No"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/rec4text3"
        app:layout_constraintEnd_toEndOf="@id/colored_bar1"
